.TH XCreateGC 3X 
.SH NAME
XCreateGC, XCopyGC, XChangeGC, XFreeGC \- create and free graphics contexts
.SH SYNTAX
.so ../Xsrc/XCreateGC.f
.LP
.so ../Xsrc/XCopyGC.f
.LP
.so ../Xsrc/XChGC.f
.LP
.so ../Xsrc/XFreeGC.f
.SH ARGUMENTS
.so ../Xsrc/d.a
.so ../Xsrc/dest.a
.so ../Xsrc/display.a
.so ../Xsrc/gc.a
.so ../Xsrc/src.a
.so ../Xsrc/valuemask3.a
.so ../Xsrc/valuemask2.a
.so ../Xsrc/valuemask1.a
.so ../Xsrc/values1.a
.SH DESCRIPTION
.so ../Xdescrip/XCreateGC.d
.ds fd .PN XCreateGC
.so ../Xsrc/errsent.com
.PN BadDrawable ,
.PN BadPixmap ,
.PN BadAlloc ,
.PN BadMatch ,
and
.PN BadFont.
.LP
.so ../Xdescrip/XCopyGC.d
.ds fd .PN XCopyGC
.so ../Xsrc/errsent.com
.PN BadGC ,
.PN BadAlloc ,
and
.PN BadValue .
.LP
.so ../Xdescrip/XChGC.d
.ds fd .PN XChangeGC
.so ../Xsrc/errsent.com
.PN BadGC ,
.PN BadPixmap ,
.PN BadFont ,
.PN BadMatch ,
.PN BadAlloc ,
and
.PN BadValue .
.LP
.so ../Xdescrip/XFreeGC.d
.LP
.ds fd .PN XFreeGC
.so ../Xsrc/errsent4.com
.PN BadGC .
.SH DIAGNOSTICS
.so ../Xerrors/badalloc.e
.so ../Xerrors/baddrawable.e
.so ../Xerrors/badfont.e
.so ../Xerrors/badgc.e
.so ../Xerrors/badmatch1.e
.so ../Xerrors/badmatch2.e
.so ../Xerrors/badpixmap.e
.so ../Xerrors/badvalue.e
.SH "SEE ALSO"
