.TH XSetTile 3X 
.SH NAME
XSetTile, XSetStipple, XSetTSOrigin \- GC convience routines
.SH SYNTAX
.so ../Xsrc/XSetTile.f
.LP
.so ../Xsrc/XSetStipple.f
.LP
.so ../Xsrc/XSetTSOrig.f
.SH ARGUMENTS
.so ../Xsrc/display.a
.so ../Xsrc/gc.a
.so ../Xsrc/stipple.a
.so ../Xsrc/tile1.a
.so ../Xsrc/tsxy.a
.SH DESCRIPTION
.so ../Xdescrip/XSetTile.d
.ds fd .PN XSetTile
.so ../Xsrc/errsent.com
.PN BadGC ,
.PN BadPixmap ,
.PN BadMatch ,
and
.PN BadAlloc .
.LP
.so ../Xdescrip/XSetStipple.d
.ds fd .PN XSetStipple
.so ../Xsrc/errsent.com
.PN BadGC ,
.PN BadPixmap ,
.PN BadMatch ,
and
.PN BadAlloc .
.LP
.so ../Xdescrip/XSetTSOrig.d
.ds fd .PN XSetTSOrigin
.so ../Xsrc/errsent4.com
.PN BadGC .
.SH DIAGNOSTICS
.so ../Xerrors/badalloc.e
.so ../Xerrors/badgc.e
.so ../Xerrors/badmatch2.e
.so ../Xerrors/badpixmap.e
.SH "SEE ALSO"
