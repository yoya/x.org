.so ../Xsrc/prop.a
.so ../Xsrc/property.a
.so ../Xsrc/properties.a
.so ../Xsrc/reqtype.a
.so ../Xsrc/type.a
.so ../Xsrc/w.a
.SH DESCRIPTION
.so ../Xdescrip/XGetProperty.d
.ds fd .PN XGetWindowProperty
.so ../Xsrc/errsent.com
.PN BadAtom ,
.PN BadValue ,
and
.PN BadWindow .
.LP
.so ../Xdescrip/XListProps.d
.ds fd .PN XListProperties
.so ../Xsrc/errsent4.com
.PN BadWindow .
.LP
.so ../Xdescrip/XChProperty.d
.ds fd .PN XChangeProperty
.so ../Xsrc/errsent.com
.PN BadWindow ,
.PN BadAtom ,
.PN BadAlloc ,
and
.PN BadValue .
.LP
.so ../Xdescrip/XRotProp.d
.ds fd .PN XRotateWindowProperties
.so ../Xsrc/errsent4.com
.PN BadWindow .
.LP
.so ../Xdescrip/XDeleteProp.d
.ds fd .PN XDeleteProperty
.so ../Xsrc/errsent.com
.PN BadWindow
and
.PN BadAtom .
.SH DIAGNOSTICS
.so ../Xerrors/badalloc.e
.so ../Xerrors/badatom.e
.so ../Xerrors/badvalue.e
.so ../Xerrors/badwindow.e
