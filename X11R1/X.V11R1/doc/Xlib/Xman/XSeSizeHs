.TH XSetSizeHints 3X 
.SH NAME
XSetSizeHints, XGetSizeHints \- set or get window size hints
.SH SYNTAX
.so ../Xsrc/XSetSHnts.f
.LP
.so ../Xsrc/XGetSHnts.f
.SH ARGUMENTS
.so ../Xsrc/display.a
.so ../Xsrc/hints1.a
.so ../Xsrc/hints1_ret.a
.so ../Xsrc/property.a
.so ../Xsrc/w.a
.SH DESCRIPTION
.so ../Xdescrip/XSetSHnts.d
.ds fd .PN XSetSizeHints
.so ../Xsrc/errsent.com
.PN BadWindow ,
.PN BadAlloc ,
and
.PN BadAtom .
.LP
.so ../Xdescrip/XGetSHnts.d
.ds fd .PN XGetSizeHints
.so ../Xsrc/errsent.com
.PN BadWindow 
and 
.PN BadAtom .
.SH DIAGNOSTICS
.so ../Xerrors/badalloc.e
.so ../Xerrors/badatom.e
.so ../Xerrors/badwindow.e
.SH "SEE ALSO"
XSetClassHint(3X),
XSetCommand(3X),
XSetIconName(3X),
XSetIconSizeHints(3X),
XSetNormalHints(3X),
XSetStandardProperties(3X),
XSetTransientForHint(3X),
XSetWMHints(3X),
XSetZoomHints(3X),
XStoreName(3X)
.br
