.so ../Xsrc/display.a
.so ../Xsrc/foreground.a
.so ../Xsrc/function.a
.so ../Xsrc/gc.a
.so ../Xsrc/plane_mask.a
.SH DESCRIPTION
.so ../Xdescrip/XSetState.d
.ds fd .PN XSetState
.so ../Xsrc/errsent.com
.PN BadGC
and
.PN BadValue .
.LP
.so ../Xdescrip/XSetFunc.d
.ds fd .PN XSetFunction
.so ../Xsrc/errsent.com
.PN BadGC 
and
.PN BadValue .
.LP
.so ../Xdescrip/XSetPlnMsk.d
.ds fd .PN XSetPlaneMask
.so ../Xsrc/errsent4.com
.PN BadGC .
.LP
.so ../Xdescrip/XSetFgrnd.d
.ds fd .PN XSetForeground
.so ../Xsrc/errsent4.com
.PN BadGC .
.LP
.so ../Xdescrip/XSetBckgrnd.d
.ds fd .PN XSetBackground
.so ../Xsrc/errsent4.com
.PN BadGC .
.SH DIAGNOSTICS
.so ../Xerrors/badgc.e
.so ../Xerrors/badvalue.e
