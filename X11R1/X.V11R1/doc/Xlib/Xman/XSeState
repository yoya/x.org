.TH XSetState 3X 
.SH NAME
XSetState, XSetFunction, XSetPlanemask, XSetForeground, XSetBackground 
\- GC convience routines
.SH SYNTAX
.so ../Xsrc/XSetState.f
.LP
.so ../Xsrc/XSetFunc.f
.LP
.so ../Xsrc/XSetPlnMsk.f
.LP
.so ../Xsrc/XSetFgrnd.f
.LP
.so ../Xsrc/XSetBckgrnd.f
.SH ARGUMENTS
