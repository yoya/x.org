# $Header: Imakefile,v 1.3 87/09/05 17:04:35 toddb Exp $
       INCLUDES = -I$(TOP) -I$(INCLUDESRC) -I$(XLIBSRC) -I../include
           SRCS = XFillZoids.c XBezier.c
           OBJS = XFillZoids.o XBezier.o

.c.o:
	$(CC) -c $(CFLAGS) $*.c

all:: libXext.a

libXext.a: $(OBJS)
	rm -f $@
	ar clq $@ $(OBJS)
	ranlib $@

lintlib:: llib-lXext.ln

llib-lXext.ln: $(SRCS)
	rm -f $@
	$(LINT) -CXext $(DEFINES) ${INCLUDES} $(SRCS)

install:: libXext.a
	$(INSTALL) $(INSTLIBFLAGS) libXext.a $(USRLIBDIR)
	ranlib $(USRLIBDIR)/libXext.a

install:: llib-lXext.ln
	$(INSTALL) $(INSTLIBFLAGS) llib-lXext.ln $(USRLIBDIR)

depend::
	$(DEPEND) -s "# DO NOT DELETE" $(CFLAGS) $(SRCS)

lint:
	$(LINT) -buz ${INCLUDES} \
		-DLIBDIR=\"${LIBDIR}\" \
		${SRCS} \
		$(XLIBSRC)/llib-lX.ln

clean::
	$(RM_CMD) \#*

Makefile:: Imakefile \
	$(UTILSRC)/imake.includes/Imake.tmpl \
	$(UTILSRC)/imake.includes/Imake.rules \
	$(UTILSRC)/imake.includes/$(MACROFILE)
		-rm -f Makefile.bak; mv Makefile Makefile.bak
		$(IMAKE_CMD) -DTOPDIR=$(TOP)

tags::
	$(TAGS) -w *.[ch]
	$(TAGS) -xw *.[ch] > TAGS

install::
	@echo "install done"

