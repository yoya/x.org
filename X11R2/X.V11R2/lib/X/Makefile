all:: libX11.a

libX11.a: $(OBJS)
	$(RM) $@
	$(AR) $@ $(OBJS)
	$(RANLIB) $@

all:: libX.a
libX.a: libX11.a
	$(RM) $@
	$(LN) libX11.a $@

lintlib:: llib-lX11.ln

llib-lX11.ln: *.c
	$(RM) $@
	$(LINT) $(LINTLIBFLAG)X11 $(LINTFLAGS) *.c

install:: libX11.a
	$(INSTALL) -c $(INSTLIBFLAGS) libX11.a $(USRLIBDIR)
	$(RANLIB) $(USRLIBDIR)/libX11.a

install::
	-(cd $(USRLIBDIR); $(LN) libX11.a libX.a)

install:: llib-lX11.ln
	$(INSTALL) -c $(INSTLIBFLAGS) llib-lX11.ln $(LINTLIBDIR)

install:: $(HEADERS)
	@case '${MFLAGS}' in *[i]*) set +e;; esac; \
	for i in $(HEADERS); do \
	(set -x; $(INSTALL) -c $(INSTALLFLAGS) $$i $(INCDIR)); \
	done

install:: XErrorDB
	$(INSTALL) -c $(INSTALLFLAGS) XErrorDB $(LIBDIR)

.c.o:
	$(RM) $@
	$(CC) -c $(CFLAGS) $*.c

XErrDes.o: $(IRULESRC)/Imake.tmpl
	$(RM) $@
	$(CC) -c $(CFLAGS) $(DB_DEFINES) $*.c

depend:: ks_names.h

depend:: $(DEPEND)

depend::
	$(DEPEND) -s "# DO NOT DELETE" $(CFLAGS) $(SRCS)

$(DEPEND):
	@echo "making $(DEPENDSRC)"; \
	cd $(DEPENDSRC); $(MAKE)

ks_names.h: $(INCLUDESRC)/keysymdef.h
	@echo "generating $@"; \
	awk 'BEGIN { \
		printf("/*\n * This is a machine generated file."); \
		printf(" * Do not put under source control.\n */\n"); \
	} \
	/^#define/ { \
		len = length($$2)-3; \
		printf("{ \"%s\", %s },\n", substr($$2,4,len), $$3); \
	}' < $? > $@

clean::
	$(RM) ks_names.h

XStrKeysym.o: ks_names.h

# structure alignment errors can safely be ignored.
# complaints about qevent can be ignored too.

lint:
	$(LINT) $(LINTFLAGS) *.c $(LINTLIBS)
lint1:
	$(LINT) $(LINTFLAGS) $(FILE) $(LINTLIBS)

clean::
	$(RM_CMD) \#*

Makefile:: $(IMAKE)

Makefile:: Imakefile \
	$(IRULESRC)/Imake.tmpl \
	$(IRULESRC)/Imake.rules \
	$(IRULESRC)/$(MACROFILE)
	-$(RM) Makefile.bak; $(MV) Makefile Makefile.bak
	$(IMAKE_CMD) -DTOPDIR=$(TOP)

$(IMAKE):
	@echo "making $(IMAKESRC)"; \
	cd $(IMAKESRC); $(MAKE)

tags::
	$(TAGS) -w *.[ch]
	$(TAGS) -xw *.[ch] > TAGS

install::
	@echo "install done"

Makefiles::

