all:: Atoms.h

Atoms.h: StringDefs.h
	$(RM) Atoms.h
	$(LN) StringDefs.h Atoms.h

.c.o:
	$(RM) $@
	$(CC) -c $(CFLAGS) $*.c

Initialize.o: $(IRULESRC)/Imake.tmpl
	$(RM) $@
	$(CC) -c $(CFLAGS) $(APP_DEFINES) $*.c

all:: libXt.a

libXt.a: $(OBJS)
	$(RM) $@
	$(AR) $@ $(OBJS)
	$(RANLIB) $@

lintlib:: llib-lXt.ln

llib-lXt.ln: $(SRCS)
	$(RM) $@
	$(LINT) $(LINTLIBFLAG)Xt $(LINTFLAGS) $(SRCS)

install:: libXt.a
	$(INSTALL) -c $(INSTLIBFLAGS) libXt.a $(USRLIBDIR)
	$(RANLIB) $(USRLIBDIR)/libXt.a

install:: llib-lXt.ln
	$(INSTALL) -c $(INSTLIBFLAGS) llib-lXt.ln $(LINTLIBDIR)

install:: $(HEADERS)
	@case '${MFLAGS}' in *[i]*) set +e;; esac; \
	for i in $(HEADERS); do \
	(set -x; $(INSTALL) -c $(INSTALLFLAGS) $$i $(INCDIR)); \
	done

depend:: Atoms.h

depend:: $(DEPEND)

depend::
	$(DEPEND) -s "# DO NOT DELETE" $(CFLAGS) $(SRCS)

$(DEPEND):
	@echo "making $(DEPENDSRC)"; \
	cd $(DEPENDSRC); $(MAKE)

lint:
	$(LINT) $(LINTFLAGS) $(SRCS) $(LINTLIBS)
lint1:
	$(LINT) $(LINTFLAGS) $(FILE) $(LINTLIBS)

clean::
	$(RM) Atoms.h

clean::
	$(RM_CMD) \#*

Makefile:: $(IMAKE)

Makefile:: Imakefile \
	$(IRULESRC)/Imake.tmpl \
	$(IRULESRC)/Imake.rules \
	$(IRULESRC)/$(MACROFILE)
	-$(RM) Makefile.bak; $(MV) Makefile Makefile.bak
	$(IMAKE_CMD) -DTOPDIR=$(TOP)

$(IMAKE):
	@echo "making $(IMAKESRC)"; \
	cd $(IMAKESRC); $(MAKE)

tags::
	$(TAGS) -w *.[ch]
	$(TAGS) -xw *.[ch] > TAGS

install::
	@echo "install done"

