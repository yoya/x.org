          SRCS1 = xhw0.c
          SRCS2 = xhw1.c
          SRCS3 = xhw3.c
          SRCS4 = hw.c
          OBJS1 = xhw0.o
          OBJS2 = xhw1.o
          OBJS3 = xhw3.o
          OBJS4 = hw.o

xhw0:  $(OBJS1)  $(XLIB)
	$(RM) $@
	$(CC) -o $@  $(OBJS1)  $(XLIB) $(LDFLAGS)

relink::
	$(RM) xhw0
	$(MAKE) $(MFLAGS) xhw0

clean::
	$(RM) xhw0

xhw1:  $(OBJS2)  $(XLIB)
	$(RM) $@
	$(CC) -o $@  $(OBJS2)  $(XLIB) $(LDFLAGS)

relink::
	$(RM) xhw1
	$(MAKE) $(MFLAGS) xhw1

clean::
	$(RM) xhw1

xhw3:  $(OBJS3)  $(XTOOLLIB) $(OLDXRMLIB) $(XLIB)
	$(RM) $@
	$(CC) -o $@  $(OBJS3)  $(XTOOLLIB) $(OLDXRMLIB) $(XLIB) $(LDFLAGS)

relink::
	$(RM) xhw3
	$(MAKE) $(MFLAGS) xhw3

clean::
	$(RM) xhw3

hw:  $(OBJS4)
	$(RM) $@
	$(CC) -o $@  $(OBJS4)  $(LDFLAGS)

relink::
	$(RM) hw
	$(MAKE) $(MFLAGS) hw

clean::
	$(RM) hw

clean::
	$(RM_CMD) \#*

Makefile:: $(IMAKE)

Makefile:: Imakefile \
	$(IRULESRC)/Imake.tmpl \
	$(IRULESRC)/Imake.rules \
	$(IRULESRC)/$(MACROFILE)
	-$(RM) Makefile.bak; $(MV) Makefile Makefile.bak
	$(IMAKE_CMD) -DTOPDIR=$(TOP)

$(IMAKE):
	@echo "making $(IMAKESRC)"; \
	cd $(IMAKESRC); $(MAKE)

tags::
	$(TAGS) -w *.[ch]
	$(TAGS) -xw *.[ch] > TAGS

install::
	@echo "install done"

