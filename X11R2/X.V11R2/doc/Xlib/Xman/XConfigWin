.TH XConfigureWindow 3X "" "1 March 1988" "X Version 11" 
.SH NAME
XConfigureWindow, XMoveWindow, XResizeWindow, XMoveResizeWindow, XSetWindowBorderWidth \- configure windows
.SH SYNTAX
.so ../Xsrc/XReconfWin.f
.LP
.so ../Xsrc/XMoveWindow.f
.LP
.so ../Xsrc/XChWindow.f
.LP
.so ../Xsrc/XConfWindow.f
.LP
.so ../Xsrc/XBdrWidth.f
.SH ARGUMENTS
.so ../Xsrc/display.a
.so ../Xsrc/value_mask.a
.so ../Xsrc/values.a
.so ../Xsrc/w.a
.so ../Xsrc/widtheight.a
.so ../Xsrc/xy.a
.SH DESCRIPTION
.so ../Xdescrip/XReconfWin.d
.ds fd .PN XConfigureWindow
.so ../Xsrc/errsent.com
.PN BadMatch ,
.PN BadValue ,
and
.PN BadWindow 
errors.
.LP
.so ../Xdescrip/XMoveWindow.d
.ds fd .PN XMoveWindow
.so ../Xsrc/errsent4.com
.PN BadWindow 
error.
.LP
.so ../Xdescrip/XChWindow.d
.ds fd .PN XResizeWindow
.so ../Xsrc/errsent4.com
.PN BadWindow 
error.
.LP
.so ../Xdescrip/XConfWindow.d
.ds fd .PN XMoveResizeWindow
.so ../Xsrc/errsent.com
.PN BadMatch ,
.PN BadValue ,
and
.PN BadWindow 
errors.
.LP
.so ../Xdescrip/XBdrWidth.d
.ds fd .PN XSetWindowBorderWidth
.so ../Xsrc/errsent.com
.PN BadValue
and
.PN BadWindow 
errors.
.SH DIAGNOSTICS
.so ../Xerrors/badmatch1.e
.so ../Xerrors/badmatch2.e
.so ../Xerrors/badvalue.e
.so ../Xerrors/badwindow.e
.SH "SEE ALSO"
