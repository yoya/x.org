.TH XSetClipOrigin 3X "" "1 March 1988" "X Version 11" 
.SH NAME
XSetClipOrigin, XSetClipMask, XSetClipRectangles \- GC convience routines
.SH SYNTAX
.so ../Xsrc/XSetClipOrig.f
.LP
.so ../Xsrc/XSetClpMask.f
.LP
.so ../Xsrc/XSetClpRects.f
.SH ARGUMENTS
.so ../Xsrc/display.a
.so ../Xsrc/clipxy.a
.so ../Xsrc/gc.a
.so ../Xsrc/n2.a
.so ../Xsrc/ordering.a
.so ../Xsrc/pixmap.a
.so ../Xsrc/rectangles1.a
.SH DESCRIPTION
.so ../Xdescrip/XSetClipOrig.d
.ds fd .PN XSetClipOrigin
.so ../Xsrc/errsent4.com
.PN BadGC 
error.
.LP
.so ../Xdescrip/XSetClpMask.d
.ds fd .PN XSetClipMask
.so ../Xsrc/errsent.com
.PN BadGC ,
.PN BadMatch ,
and
.PN BadValue 
errors.
.LP
.so ../Xdescrip/XSetClpRects.d
.ds fd .PN XSetClipRectangles
.so ../Xsrc/errsent.com
.PN BadAlloc ,
.PN BadGC ,
.PN BadMatch ,
and
.PN BadValue 
errors.
.SH DIAGNOSTICS
.so ../Xerrors/badalloc.e
.so ../Xerrors/badgc.e
.so ../Xerrors/badmatch2.e
.so ../Xerrors/badvalue.e
.SH "SEE ALSO"
