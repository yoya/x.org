.so ../Xsrc/display.a
.so ../Xsrc/w_save_set.a
.so ../Xsrc/w_save_set1.a
.SH DESCRIPTION
.so ../Xdescrip/XChSaveSet.d
.ds fd .PN XChangeSaveSet
.so ../Xsrc/errsent.com
.PN BadMatch ,
.PN BadValue ,
and
.PN BadWindow 
errors.
.LP
.so ../Xdescrip/XAddSaveSet.d
.ds fd .PN XAddToSaveSet
.so ../Xsrc/errsent.com
.PN BadMatch 
and
.PN BadWindow 
errors.
.LP
.so ../Xdescrip/XRmvSaveSet.d
.ds fd .PN XRemoveFromSaveSet
.so ../Xsrc/errsent.com
.PN BadMatch
and
.PN BadWindow 
errors.
.SH DIAGNOSTICS
.so ../Xerrors/badmatch2.e
