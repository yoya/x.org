.TH XDrawRectangle 3X "" "1 March 1988" "X Version 11" 
.SH NAME
XDrawRectangle, XDrawRectangles \- draw rectyangles
.SH SYNTAX
.so ../Xsrc/XRectangle.f
.LP
.so ../Xsrc/XRectangles.f
.SH ARGUMENTS
.so ../Xsrc/d.a
.so ../Xsrc/display.a
.so ../Xsrc/gc.a
.so ../Xsrc/nrectangles.a
.so ../Xsrc/rectangles.a
.so ../Xsrc/widtheight4.a
.so ../Xsrc/xy_rect.a
.SH DESCRIPTION
.so ../Xdescrip/XRectangle.d
.ds fd .PN XDrawRectangle
.ds fe .PN XDrawRectangles
.so ../Xsrc/errsent3.com
.PN BadDrawable ,
.PN BadGC ,
and
.PN BadMatch 
errors.
.SH DIAGNOSTICS
.so ../Xerrors/baddrawable.e
.so ../Xerrors/badgc.e
.so ../Xerrors/badmatch1.e
.so ../Xerrors/badmatch2.e
