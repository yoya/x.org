.so ../Xsrc/back_pix.a
.so ../Xsrc/tile.a
.so ../Xsrc/bord_pix.a
.so ../Xsrc/bord_pixmap.a
.so ../Xsrc/display.a
.so ../Xsrc/valuemask.a
.so ../Xsrc/w.a
.SH DESCRIPTION
.so ../Xdescrip/XChWAttr.d
.ds fd .PN XChangeWindowAttributes
.so ../Xsrc/errsent.com
.PN BadAccess ,
.PN BadColor ,
.PN BadCursor ,
.PN BadMatch ,
.PN BadPixmap ,
.PN BadValue ,
and
.PN BadWindow 
errors.
.LP
