.ds fd .PN XSetWindowBackgroundPixmap
.so ../Xsrc/errsent.com
.PN BadColor ,
.PN BadMatch ,
.PN BadPixmap ,
and
.PN BadWindow 
errors.
.LP 
.so ../Xdescrip/XBorder.d
.ds fd .PN XSetWindowBorder
.so ../Xsrc/errsent.com
.PN BadMatch ,
.PN BadPixmap ,
.PN BadValue ,
and
.PN BadWindow 
errors.
.LP
.so ../Xdescrip/XPmapBorder.d
.ds fd .PN XSetWindowBorderPixmap
.so ../Xsrc/errsent.com
.PN BadMatch ,
.PN BadPixmap ,
.PN BadValue ,
and
.PN BadWindow 
errors.
