.TH XSetLineAttribute 3X "" "1 March 1988" "X Version 11" 
.SH NAME
XSetLineAttribute, XSetDashes \- GC convience routines
.SH SYNTAX
.so ../Xsrc/XSetLnStle.f
.LP
.so ../Xsrc/XSetDashes.f
.SH ARGUMENTS
.so ../Xsrc/cap_style.a
.so ../Xsrc/dashlist.a
.so ../Xsrc/dashoff.a
.so ../Xsrc/display.a
.so ../Xsrc/gc.a
.so ../Xsrc/join_style.a
.so ../Xsrc/line_style.a
.so ../Xsrc/line_width.a
.so ../Xsrc/n3.a
.SH DESCRIPTION
.so ../Xdescrip/XSetLnStle.d
.ds fd .PN XSetLineAttributes
.so ../Xsrc/errsent.com
.PN BadGC 
and
.PN BadValue 
errors.
.LP
.so ../Xdescrip/XSetDashes.d
.ds fd .PN XSetDashes
.so ../Xsrc/errsent.com
.PN BadAlloc ,
.PN BadGC ,
and
.PN BadValue 
errors.
.SH DIAGNOSTICS
.so ../Xerrors/badalloc.e
.so ../Xerrors/badgc.e
.so ../Xerrors/badvalue.e
.SH "SEE ALSO"
