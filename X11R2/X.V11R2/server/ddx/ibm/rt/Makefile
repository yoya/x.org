.c.o:
	$(RM) $@
	$(CC) -c $(CFLAGS) $*.c

all::	$(OTHROBJS)

all:: rt.o

rt.o: $(LIBOBJS)
	$(RM) $@
	$(LD) -X -r $(LIBOBJS) -o $@

all:: librtalloc.a

librtalloc.a: $(ALLOCOBJS)
	$(RM) $@
	$(AR) $@ $(ALLOCOBJS)
	$(RANLIB) $@

lint:
	$(LINT) $(LINTFLAGS) $(SRCS) $(LINTLIBS)
lint1:
	$(LINT) $(LINTFLAGS) $(FILE) $(LINTLIBS)

lintlib:

depend:: $(DEPEND)

depend::
	$(DEPEND) -s "# DO NOT DELETE" $(CFLAGS) $(SRCS)

$(DEPEND):
	@echo "making $(DEPENDSRC)"; \
	cd $(DEPENDSRC); $(MAKE)

rtscreen.o rtinit.o rtio.o rtutils.o:	Makefile

clean::
	$(RM_CMD) \#*

Makefile:: $(IMAKE)

Makefile:: Imakefile \
	$(IRULESRC)/Imake.tmpl \
	$(IRULESRC)/Imake.rules \
	$(IRULESRC)/$(MACROFILE)
	-$(RM) Makefile.bak; $(MV) Makefile Makefile.bak
	$(IMAKE_CMD) -DTOPDIR=$(TOP)

$(IMAKE):
	@echo "making $(IMAKESRC)"; \
	cd $(IMAKESRC); $(MAKE)

tags::
	$(TAGS) -w *.[ch]
	$(TAGS) -xw *.[ch] > TAGS

install::
	@echo "install done"

