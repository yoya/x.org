# DO NOT DELETE THIS LINE -- make depend depends on it.

SRCS = util.c
OBJS = util.o
STD_DEFINES =  -DSUN_WINDOWS -DCOMPRESSED_FONTS
DEFINES=  -DTCPCONN -DUNIXCONN
CDEBUGFLAGS = -O
INCLUDES = -I.  -I../../include -I$(INCLUDESRC)
LINTLIBS = ../../dix/llib-ldix.ln

.c.o:
	$(RM) $@
	$(CC) -c $(CFLAGS) $*.c

all:: libos.a

libos.a: $(OBJS)
	$(RM) $@
	$(AR) $@ $(OBJS)
	$(RANLIB) $@

lintlib:: llib-los.ln

llib-los.ln: $(SRCS)
	$(RM) $@
	$(LINT) $(LINTLIBFLAG)os $(LINTFLAGS) $(SRCS)

lint:
	$(LINT) $(LINTFLAGS) $(SRCS) $(LINTLIBS)
lint1:
	$(LINT) $(LINTFLAGS) $(FILE) $(LINTLIBS)

depend:: $(DEPEND)

depend::
	$(DEPEND) -s "# DO NOT DELETE" $(CFLAGS) $(SRCS)

$(DEPEND):
	@echo "making $(DEPENDSRC)"; \
	cd $(DEPENDSRC); $(MAKE)

clean::
	$(RM_CMD) \#*

Makefile:: $(IMAKE)

Makefile:: Imakefile \
	$(IRULESRC)/Imake.tmpl \
	$(IRULESRC)/Imake.rules \
	$(IRULESRC)/$(MACROFILE)
	-$(RM) Makefile.bak; $(MV) Makefile Makefile.bak
	$(IMAKE_CMD) -DTOPDIR=$(TOP)

$(IMAKE):
	@echo "making $(IMAKESRC)"; \
	cd $(IMAKESRC); $(MAKE)

tags::
	$(TAGS) -w *.[ch]
	$(TAGS) -xw *.[ch] > TAGS

install::
	@echo "install done"

Makefiles::

