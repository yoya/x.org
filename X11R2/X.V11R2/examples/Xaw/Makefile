xtext: xtext.o $(LOCAL_LIBRARIES)
	$(RM) $@
	$(CC) -o $@ xtext.o $(LOCAL_LIBRARIES) $(LDFLAGS)

relink::
	$(RM) xtext
	$(MAKE) $(MFLAGS) xtext

clean::
